<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListeMesAnnoncesActivity">

    <TextView
        android:id="@+id/liste_mes_annonces_tv_listeannonces"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/liste_mes_annonces_tv_listeannonces"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/liste_mes_annonces_lv_listeannonces"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/liste_mes_annonces_tv_listeannonces" />

</android.support.constraint.ConstraintLayout>